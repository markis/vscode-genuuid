{
  "name": "genuuid",
  "displayName": "Insert UUID/GUID",
  "description": "Simple Insert UUID/GUID. Nothing fancy.",
  "icon": "images/icon.png",
  "version": "1.0.0",
  "publisher": "markis",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/markis/vscode-genuuid.git"
  },
  "engines": {
    "vscode": "^1.13.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.insertuuid"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.insertuuid",
        "title": "Insert UUID/GUID"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  }
}
